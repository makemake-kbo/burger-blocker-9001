{
	"name": "Euro Blocker 9000",
	"version": "0.1.0",
	"description": "English only USA",
	"permissions":["storage"],
	"browser_action":
	{
		"icons":
		{
			"128": "icon.png"
		},
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"js": ["content.js"],
			"matches": ["https://www.teamfortress.tv/*"]
		}
	],

	"manifest_version": 2
}